{
  "manifest_version": 2,
  "name": "Katame",
  "description": "This extension will do something",
  "version": "1.0",
  "browser_action": {
   "default_icon": "images/icon.png"
  },
  "background" : {
    "scripts" : ["scripts/background.js"]
  },
  "web_accessible_resources" : [
    "layouts/menu.html"
  ],
  "permissions": [
    "tabs", "<all_urls>"
  ]
}